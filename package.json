{
  "name": "jsw",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build && pnpm run postbuild:wasmopt",
    "postbuild:wasmopt": "command -v wasm-opt >/dev/null 2>&1 && wasm-opt public/jsw.wasm -O3 --vacuum --dce --inlining-optimizing --enable-simd --enable-bulk-memory --enable-mutable-globals --enable-nontrapping-float-to-int -o public/jsw.opt.wasm && mv public/jsw.opt.wasm public/jsw.wasm || echo 'wasm-opt not found, skipping wasm-opt'",
    "preview": "vite preview"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.22.0",
  "devDependencies": {
    "@babel/generator": "^7.28.5",
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "assemblyscript": "^0.28.9",
    "astring": "^1.9.0",
    "glob": "^13.0.0",
    "typescript": "^5.9.3",
    "vite": "^6.4.1"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./build/release.js",
      "types": "./build/release.d.ts"
    }
  },
  "wasmOpt": {
    "args": [
      "--enable-bulk-memory",
      "--enable-mutable-globals",
      "--enable-nontrapping-float-to-int"
    ]
  },
  "dependencies": {
    "@assemblyscript/loader": "^0.28.9"
  }
}